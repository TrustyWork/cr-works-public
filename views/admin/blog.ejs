<div class="col-md-6">
    <h1>
        Blogger rss widget: blog
    </h1>

    <form action="" method="post">

        <div class="form-group">
            <label for="category-input">Category</label>
            <select class="form-control" id="category-input" name="category">
                <% for( var idx in category) {%>
                <option value="<%= idx%>"><%= category[ idx]%></option>
                <% } %>
            </select>
        </div>


        <div class="form-group">
            <label for="lang-input">Language</label>
            <select class="form-control" id="lang-input" name="lang">
                <option value="en">EN</option>
                <option value="ru">RU</option>
            </select>
        </div>


        <div class="form-group">
            <label for="uri-input">Add uri</label>
            <input type="text" class="form-control" id="uri-input" name="uri">
        </div>

        <input type="submit" class="form-control">
    </form>

</div>
<div class="col-md-6">

</div>